    { "schemaName": "description", "displayName": "Description", "dataType": "MultilineText", "required": false, "description": "Report description" },
    { "schemaName": "query_definition_json", "displayName": "Query Definition", "dataType": "MultilineText", "required": true, "description": "Query config JSON" },
