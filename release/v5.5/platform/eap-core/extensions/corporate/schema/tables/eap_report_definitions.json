    { "schemaName": "output_format", "displayName": "Output Format", "dataType": "Choice", "required": true, "defaultValue": "Table", "options": ["Table", "Chart", "Mixed", "Custom"], "description": "Output format" },
    { "schemaName": "visualization_config_json", "displayName": "Visualization Config", "dataType": "MultilineText", "required": false, "description": "Viz config JSON" },
