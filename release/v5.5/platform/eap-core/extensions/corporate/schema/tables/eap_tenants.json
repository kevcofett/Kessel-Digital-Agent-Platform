{
  "schemaName": "eap_tenants",
  "displayName": "Tenants",
  "pluralName": "Tenants",
  "description": "Tenant definitions for multi-tenant deployment",
  "tier": 3,
  "featureGate": "MULTI_TENANT",
  "category": "MultiTenant",
  "primaryKey": "tenant_id",
  "primaryNameColumn": "tenant_name",
  "ownershipType": "OrganizationOwned",
  "columns": [
    { "schemaName": "tenant_id", "displayName": "Tenant ID", "dataType": "UniqueIdentifier", "required": true, "description": "Primary key" },
    { "schemaName": "tenant_code", "displayName": "Tenant Code", "dataType": "Text", "maxLength": 50, "required": true, "unique": true, "description": "Unique tenant code" },
    { "schemaName": "tenant_name", "displayName": "Tenant Name", "dataType": "Text", "maxLength": 100, "required": true, "description": "Tenant name" },
    { "schemaName": "tenant_type", "displayName": "Tenant Type", "dataType": "Choice", "required": true, "options": ["Internal", "Client"], "description": "Tenant type" },
    { "schemaName": "client_id", "displayName": "Client", "dataType": "Lookup", "lookupTable": "eap_client", "required": false, "description": "Client (if client tenant)" },
    { "schemaName": "management_model", "displayName": "Management Model", "dataType": "Choice", "required": false, "options": ["VendorManaged", "ClientManaged", "Hybrid"], "description": "Management model" },
    { "schemaName": "dataverse_environment_id", "displayName": "Dataverse Environment", "dataType": "Text", "maxLength": 100, "required": false, "description": "Dataverse environment ID" },
    { "schemaName": "sharepoint_site_url", "displayName": "SharePoint Site URL", "dataType": "Text", "maxLength": 500, "required": false, "description": "SharePoint site URL" },
    { "schemaName": "search_index_prefix", "displayName": "Search Index Prefix", "dataType": "Text", "maxLength": 50, "required": false, "description": "AI Search index prefix" },
    { "schemaName": "license_type", "displayName": "License Type", "dataType": "Choice", "required": false, "options": ["Enterprise", "Professional", "Basic", "Trial"], "description": "License type" },
    { "schemaName": "licensed_users_count", "displayName": "Licensed Users", "dataType": "WholeNumber", "required": false, "description": "Licensed user count" },
    { "schemaName": "monthly_token_limit", "displayName": "Monthly Token Limit", "dataType": "WholeNumber", "required": false, "description": "Monthly token limit" },
    { "schemaName": "storage_limit_gb", "displayName": "Storage Limit (GB)", "dataType": "Decimal", "required": false, "description": "Storage limit in GB" },
    { "schemaName": "current_token_usage", "displayName": "Current Token Usage", "dataType": "WholeNumber", "required": false, "description": "Current token usage" },
    { "schemaName": "current_storage_usage_gb", "displayName": "Current Storage (GB)", "dataType": "Decimal", "required": false, "description": "Current storage in GB" },
    { "schemaName": "status", "displayName": "Status", "dataType": "Choice", "required": true, "defaultValue": "Active", "options": ["Active", "Suspended", "Trial", "Terminated"], "description": "Tenant status" },
    { "schemaName": "trial_end_date", "displayName": "Trial End Date", "dataType": "DateTime", "required": false, "description": "Trial expiration" },
    { "schemaName": "contract_start_date", "displayName": "Contract Start", "dataType": "DateTime", "required": false, "description": "Contract start date" },
    { "schemaName": "contract_end_date", "displayName": "Contract End", "dataType": "DateTime", "required": false, "description": "Contract end date" },
    { "schemaName": "primary_contact_user_id", "displayName": "Primary Contact", "dataType": "Lookup", "lookupTable": "eap_user", "required": false, "description": "Primary contact" },
    { "schemaName": "billing_contact_email", "displayName": "Billing Contact Email", "dataType": "Text", "maxLength": 200, "required": false, "description": "Billing email" },
    { "schemaName": "is_active", "displayName": "Is Active", "dataType": "Boolean", "required": true, "defaultValue": true, "description": "Active status" },
    { "schemaName": "created_date", "displayName": "Created Date", "dataType": "DateTime", "required": true, "description": "Record created" },
    { "schemaName": "created_by", "displayName": "Created By", "dataType": "Lookup", "lookupTable": "eap_user", "required": true, "description": "Creator" },
    { "schemaName": "modified_date", "displayName": "Modified Date", "dataType": "DateTime", "required": true, "description": "Last modified" },
    { "schemaName": "modified_by", "displayName": "Modified By", "dataType": "Lookup", "lookupTable": "eap_user", "required": true, "description": "Modifier" }
  ],
  "indexes": [
    { "name": "idx_tenant_code", "columns": ["tenant_code"], "unique": true },
    { "name": "idx_client", "columns": ["client_id"], "unique": false },
    { "name": "idx_status", "columns": ["status"], "unique": false }
  ]
}
