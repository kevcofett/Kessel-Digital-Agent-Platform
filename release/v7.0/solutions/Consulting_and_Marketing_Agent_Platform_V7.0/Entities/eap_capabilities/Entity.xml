<?xml version="1.0" encoding="utf-8"?>
<Entity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>eap_capabilities</Name>
  <EntityInfo>
    <entity Name="eap_capabilities">
      <DisplayName>Capability</DisplayName>
      <DisplayCollectionName>Capabilities</DisplayCollectionName>
      <Description>Defines capabilities available across MCMAP agents with their requirements and specifications.</Description>
      <OwnershipTypeMask>OrganizationOwned</OwnershipTypeMask>
      <IsActivity>false</IsActivity>
      <IsAvailableOffline>true</IsAvailableOffline>
      <IsCustomizable>1</IsCustomizable>
      <IsRenameable>1</IsRenameable>
      <CanCreateAttributes>1</CanCreateAttributes>
      <CanCreateForms>1</CanCreateForms>
      <CanCreateViews>1</CanCreateViews>
      <CanModifyAdditionalSettings>1</CanModifyAdditionalSettings>
      <PrimaryIdAttribute>eap_capabilityid</PrimaryIdAttribute>
      <PrimaryNameAttribute>eap_name</PrimaryNameAttribute>
      <IntroducedVersion>7.0.0.0</IntroducedVersion>
      <localizedNames>
        <localizedName description="Capability" languagecode="1033" />
      </localizedNames>
      <Descriptions>
        <Description description="Defines capabilities available across MCMAP agents" languagecode="1033" />
      </Descriptions>
      <attributes>
        <attribute PhysicalName="eap_capabilityid">
          <Type>primarykey</Type>
          <DisplayName>Capability ID</DisplayName>
          <Description>Unique identifier for the capability</Description>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_name">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Capability Name</DisplayName>
          <Description>Full name of the capability</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_code">
          <Type>nvarchar</Type>
          <MaxLength>50</MaxLength>
          <DisplayName>Capability Code</DisplayName>
          <Description>Unique code for the capability (e.g., ANL_PROJECTION)</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_description">
          <Type>nvarchar</Type>
          <MaxLength>2000</MaxLength>
          <DisplayName>Description</DisplayName>
          <Description>Full description of what the capability does</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_category">
          <Type>picklist</Type>
          <DisplayName>Category</DisplayName>
          <Description>Category of the capability</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="analytics" />
              <Option value="100000001" label="audience" />
              <Option value="100000002" label="channel" />
              <Option value="100000003" label="document" />
              <Option value="100000004" label="performance" />
              <Option value="100000005" label="consulting" />
              <Option value="100000006" label="growth" />
              <Option value="100000007" label="orchestration" />
              <Option value="100000008" label="deployment" />
            </Options>
          </OptionSet>
        </attribute>

        <attribute PhysicalName="eap_inputs">
          <Type>ntext</Type>
          <DisplayName>Inputs</DisplayName>
          <Description>JSON schema defining required inputs</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
        </attribute>

        <attribute PhysicalName="eap_outputs">
          <Type>ntext</Type>
          <DisplayName>Outputs</DisplayName>
          <Description>JSON schema defining expected outputs</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
        </attribute>

        <attribute PhysicalName="eap_prerequisites">
          <Type>nvarchar</Type>
          <MaxLength>500</MaxLength>
          <DisplayName>Prerequisites</DisplayName>
          <Description>Comma-separated list of capability codes that must be run first</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_version">
          <Type>nvarchar</Type>
          <MaxLength>20</MaxLength>
          <DisplayName>Version</DisplayName>
          <Description>Version of the capability definition</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_is_active">
          <Type>bit</Type>
          <DisplayName>Is Active</DisplayName>
          <Description>Whether the capability is currently active</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <DefaultValue>1</DefaultValue>
        </attribute>

        <attribute PhysicalName="createdon">
          <Type>datetime</Type>
          <DisplayName>Created On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="modifiedon">
          <Type>datetime</Type>
          <DisplayName>Modified On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statecode">
          <Type>state</Type>
          <DisplayName>Status</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statuscode">
          <Type>status</Type>
          <DisplayName>Status Reason</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>
      </attributes>
    </entity>
  </EntityInfo>
</Entity>
