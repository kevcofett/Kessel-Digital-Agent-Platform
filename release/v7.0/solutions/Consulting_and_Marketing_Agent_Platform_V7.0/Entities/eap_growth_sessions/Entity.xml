<?xml version="1.0" encoding="utf-8"?>
<Entity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>eap_growth_sessions</Name>
  <EntityInfo>
    <entity Name="eap_growth_sessions">
      <DisplayName>Growth Sessions</DisplayName>
      <DisplayCollectionName>Growth Sessions</DisplayCollectionName>
      <Description>Stores growth strategy session state for GHA (Growth Hacking Agent) workflows. Links to main eap_sessions table and tracks 8-step growth workflow progress.</Description>
      <OwnershipTypeMask>UserOwned</OwnershipTypeMask>
      <IsActivity>false</IsActivity>
      <IsAvailableOffline>true</IsAvailableOffline>
      <IsCustomizable>1</IsCustomizable>
      <IsRenameable>1</IsRenameable>
      <CanCreateAttributes>1</CanCreateAttributes>
      <CanCreateForms>1</CanCreateForms>
      <CanCreateViews>1</CanCreateViews>
      <CanModifyAdditionalSettings>1</CanModifyAdditionalSettings>
      <IconLargeName>eap_growth_sessions_large.png</IconLargeName>
      <IconMediumName>eap_growth_sessions_medium.png</IconMediumName>
      <IconSmallName>eap_growth_sessions_small.png</IconSmallName>
      <PrimaryIdAttribute>eap_growth_sessionid</PrimaryIdAttribute>
      <PrimaryNameAttribute>eap_name</PrimaryNameAttribute>
      <localizedNames>
        <localizedName description="Growth Session" languagecode="1033" />
      </localizedNames>
      <Descriptions>
        <Description description="Stores growth strategy session state for GHA workflows" languagecode="1033" />
      </Descriptions>
      <attributes>
        <!-- Primary Key -->
        <attribute PhysicalName="eap_growth_sessionid">
          <Type>primarykey</Type>
          <DisplayName>Growth Session ID</DisplayName>
          <Description>Unique identifier for the growth session</Description>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <!-- Primary Name -->
        <attribute PhysicalName="eap_name">
          <Type>nvarchar</Type>
          <MaxLength>200</MaxLength>
          <DisplayName>Name</DisplayName>
          <Description>Display name for the growth session</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <IsPrimaryName>1</IsPrimaryName>
        </attribute>

        <!-- Session Link -->
        <attribute PhysicalName="eap_session_id">
          <Type>lookup</Type>
          <DisplayName>Session</DisplayName>
          <Description>Link to the main eap_sessions table</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <LookupStyle>single</LookupStyle>
          <Targets>
            <Target>eap_sessions</Target>
          </Targets>
        </attribute>

        <!-- Growth Workflow Step (1-8) -->
        <attribute PhysicalName="eap_growth_workflow_step">
          <Type>int</Type>
          <DisplayName>Growth Workflow Step</DisplayName>
          <Description>Current step in the 8-step growth workflow. 1=Growth Context, 2=North Star Definition, 3=Lifecycle Analysis, 4=Audience Strategy, 5=Tactics Development, 6=Channel Strategy, 7=Projections, 8=Documentation</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <MinValue>1</MinValue>
          <MaxValue>8</MaxValue>
          <DefaultValue>1</DefaultValue>
        </attribute>

        <!-- Growth Workflow Gate (0-4) -->
        <attribute PhysicalName="eap_growth_workflow_gate">
          <Type>int</Type>
          <DisplayName>Growth Workflow Gate</DisplayName>
          <Description>Current gate in growth workflow. 0=Pre-Foundation, 1=Growth Foundation (Steps 1-2), 2=Growth Strategy (Steps 3-4), 3=Growth Tactics (Steps 5-7), 4=Growth Plan Complete (Step 8)</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <MinValue>0</MinValue>
          <MaxValue>4</MaxValue>
          <DefaultValue>0</DefaultValue>
        </attribute>

        <!-- North Star Metric -->
        <attribute PhysicalName="eap_north_star_metric">
          <Type>ntext</Type>
          <DisplayName>North Star Metric</DisplayName>
          <Description>JSON object containing the selected North Star metric with baseline and target values</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
        </attribute>

        <!-- Priority Stages -->
        <attribute PhysicalName="eap_priority_stages">
          <Type>ntext</Type>
          <DisplayName>Priority Stages</DisplayName>
          <Description>JSON array of priority AARRR lifecycle stages (acquisition, activation, retention, referral, revenue)</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
        </attribute>

        <!-- Growth Plan State -->
        <attribute PhysicalName="eap_growth_plan_state">
          <Type>ntext</Type>
          <DisplayName>Growth Plan State</DisplayName>
          <Description>JSON object containing the full growth plan state including business_model, growth_stage, lifecycle_analysis, audience_segments, tactics, channel_strategy, projections, growth_plan_document, handoff_context</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
          <MaxLength>1048576</MaxLength>
        </attribute>

        <!-- Business Model -->
        <attribute PhysicalName="eap_business_model">
          <Type>picklist</Type>
          <DisplayName>Business Model</DisplayName>
          <Description>Business model type for growth context</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="SaaS" />
              <Option value="100000001" label="Marketplace" />
              <Option value="100000002" label="Fintech" />
              <Option value="100000003" label="E-commerce" />
              <Option value="100000004" label="Consumer App" />
              <Option value="100000005" label="B2B Services" />
              <Option value="100000006" label="Media/Content" />
              <Option value="100000007" label="Other" />
            </Options>
          </OptionSet>
        </attribute>

        <!-- Growth Stage -->
        <attribute PhysicalName="eap_growth_stage">
          <Type>picklist</Type>
          <DisplayName>Growth Stage</DisplayName>
          <Description>Current growth stage of the business</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="Pre-Product-Market-Fit" />
              <Option value="100000001" label="Product-Market-Fit" />
              <Option value="100000002" label="Growth" />
              <Option value="100000003" label="Scale" />
              <Option value="100000004" label="Maturity" />
            </Options>
          </OptionSet>
        </attribute>

        <!-- Handoff Status -->
        <attribute PhysicalName="eap_handoff_status">
          <Type>picklist</Type>
          <DisplayName>Handoff Status</DisplayName>
          <Description>Status of handoff to downstream domains (MPA, CA)</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="Not Ready" />
              <Option value="100000001" label="Ready for Handoff" />
              <Option value="100000002" label="Handed off to MPA" />
              <Option value="100000003" label="Handed off to CA" />
              <Option value="100000004" label="Completed" />
            </Options>
          </OptionSet>
        </attribute>

        <!-- Specialist Calls Count -->
        <attribute PhysicalName="eap_specialist_calls_count">
          <Type>int</Type>
          <DisplayName>Specialist Calls Count</DisplayName>
          <Description>Number of specialist agent calls made during this growth session</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <MinValue>0</MinValue>
          <MaxValue>1000</MaxValue>
          <DefaultValue>0</DefaultValue>
        </attribute>

        <!-- Last Updated Step -->
        <attribute PhysicalName="eap_last_updated_step">
          <Type>datetime</Type>
          <DisplayName>Last Updated Step</DisplayName>
          <Description>Timestamp when workflow step was last updated</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>DateAndTime</Format>
        </attribute>

        <!-- Standard fields -->
        <attribute PhysicalName="createdon">
          <Type>datetime</Type>
          <DisplayName>Created On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="modifiedon">
          <Type>datetime</Type>
          <DisplayName>Modified On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statecode">
          <Type>state</Type>
          <DisplayName>Status</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statuscode">
          <Type>status</Type>
          <DisplayName>Status Reason</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>
      </attributes>
    </entity>
  </EntityInfo>
</Entity>
