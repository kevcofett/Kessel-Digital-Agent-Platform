<?xml version="1.0" encoding="utf-8"?>
<Entity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>eap_telemetry</Name>
  <EntityInfo>
    <entity Name="eap_telemetry">
      <DisplayName>Telemetry</DisplayName>
      <DisplayCollectionName>Telemetry Records</DisplayCollectionName>
      <Description>Stores agent telemetry events for MCMAP platform monitoring, performance tracking, and analytics.</Description>
      <OwnershipTypeMask>OrganizationOwned</OwnershipTypeMask>
      <IsActivity>false</IsActivity>
      <IsAvailableOffline>false</IsAvailableOffline>
      <IsCustomizable>1</IsCustomizable>
      <IsRenameable>1</IsRenameable>
      <CanCreateAttributes>1</CanCreateAttributes>
      <CanCreateForms>1</CanCreateForms>
      <CanCreateViews>1</CanCreateViews>
      <CanModifyAdditionalSettings>1</CanModifyAdditionalSettings>
      <PrimaryIdAttribute>eap_telemetryid</PrimaryIdAttribute>
      <PrimaryNameAttribute>eap_name</PrimaryNameAttribute>
      <IntroducedVersion>7.0.0.0</IntroducedVersion>
      <localizedNames>
        <localizedName description="Telemetry" languagecode="1033" />
      </localizedNames>
      <Descriptions>
        <Description description="Stores agent telemetry events for MCMAP platform" languagecode="1033" />
      </Descriptions>
      <attributes>
        <attribute PhysicalName="eap_telemetryid">
          <Type>primarykey</Type>
          <DisplayName>Telemetry ID</DisplayName>
          <Description>Unique identifier for the telemetry record</Description>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_name">
          <Type>nvarchar</Type>
          <MaxLength>200</MaxLength>
          <DisplayName>Event Name</DisplayName>
          <Description>Name of the telemetry event</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_session_id">
          <Type>lookup</Type>
          <DisplayName>Session</DisplayName>
          <Description>Link to the session that generated this telemetry</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <LookupStyle>single</LookupStyle>
          <Targets>
            <Target>eap_sessions</Target>
          </Targets>
        </attribute>

        <attribute PhysicalName="eap_agent_code">
          <Type>nvarchar</Type>
          <MaxLength>10</MaxLength>
          <DisplayName>Agent Code</DisplayName>
          <Description>Three-letter code of the agent that generated this event (ORC, ANL, AUD, etc.)</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_event_type">
          <Type>picklist</Type>
          <DisplayName>Event Type</DisplayName>
          <Description>Type of telemetry event</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="agent_invoked" />
              <Option value="100000001" label="capability_executed" />
              <Option value="100000002" label="handoff_initiated" />
              <Option value="100000003" label="handoff_completed" />
              <Option value="100000004" label="kb_accessed" />
              <Option value="100000005" label="prompt_executed" />
              <Option value="100000006" label="workflow_started" />
              <Option value="100000007" label="workflow_completed" />
              <Option value="100000008" label="error_occurred" />
              <Option value="100000009" label="gate_passed" />
              <Option value="100000010" label="user_feedback" />
              <Option value="100000011" label="document_generated" />
              <Option value="100000012" label="data_exported" />
            </Options>
          </OptionSet>
        </attribute>

        <attribute PhysicalName="eap_event_data">
          <Type>ntext</Type>
          <DisplayName>Event Data</DisplayName>
          <Description>JSON object containing event-specific data and metadata</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
          <MaxLength>1048576</MaxLength>
        </attribute>

        <attribute PhysicalName="eap_duration_ms">
          <Type>int</Type>
          <DisplayName>Duration (ms)</DisplayName>
          <Description>Duration of the event in milliseconds</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <MinValue>0</MinValue>
          <MaxValue>2147483647</MaxValue>
        </attribute>

        <attribute PhysicalName="eap_success">
          <Type>bit</Type>
          <DisplayName>Success</DisplayName>
          <Description>Whether the event completed successfully</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <DefaultValue>1</DefaultValue>
        </attribute>

        <attribute PhysicalName="eap_error_message">
          <Type>nvarchar</Type>
          <MaxLength>4000</MaxLength>
          <DisplayName>Error Message</DisplayName>
          <Description>Error message if the event failed</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_user_id">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>User ID</DisplayName>
          <Description>ID of the user who triggered the event</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_client_id">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Client ID</DisplayName>
          <Description>ID of the client context</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_correlation_id">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Correlation ID</DisplayName>
          <Description>Correlation ID for tracing related events</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_timestamp">
          <Type>datetime</Type>
          <DisplayName>Timestamp</DisplayName>
          <Description>Timestamp when the event occurred</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>DateAndTime</Format>
        </attribute>

        <attribute PhysicalName="createdon">
          <Type>datetime</Type>
          <DisplayName>Created On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="modifiedon">
          <Type>datetime</Type>
          <DisplayName>Modified On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statecode">
          <Type>state</Type>
          <DisplayName>Status</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statuscode">
          <Type>status</Type>
          <DisplayName>Status Reason</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>
      </attributes>
    </entity>
  </EntityInfo>
</Entity>
