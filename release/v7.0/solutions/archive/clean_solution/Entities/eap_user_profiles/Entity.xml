<?xml version="1.0" encoding="utf-8"?>
<Entity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>eap_user_profiles</Name>
  <EntityInfo>
    <entity Name="eap_user_profiles">
      <DisplayName>User Profile</DisplayName>
      <DisplayCollectionName>User Profiles</DisplayCollectionName>
      <Description>Stores MCMAP user profiles with preferences, settings, and platform-specific data.</Description>
      <OwnershipTypeMask>UserOwned</OwnershipTypeMask>
      <IsActivity>false</IsActivity>
      <IsAvailableOffline>true</IsAvailableOffline>
      <IsCustomizable>1</IsCustomizable>
      <IsRenameable>1</IsRenameable>
      <CanCreateAttributes>1</CanCreateAttributes>
      <CanCreateForms>1</CanCreateForms>
      <CanCreateViews>1</CanCreateViews>
      <CanModifyAdditionalSettings>1</CanModifyAdditionalSettings>
      <PrimaryIdAttribute>eap_user_profileid</PrimaryIdAttribute>
      <PrimaryNameAttribute>eap_name</PrimaryNameAttribute>
      <IntroducedVersion>7.0.0.0</IntroducedVersion>
      <localizedNames>
        <localizedName description="User Profile" languagecode="1033" />
      </localizedNames>
      <Descriptions>
        <Description description="Stores MCMAP user profiles" languagecode="1033" />
      </Descriptions>
      <attributes>
        <attribute PhysicalName="eap_user_profileid">
          <Type>primarykey</Type>
          <DisplayName>User Profile ID</DisplayName>
          <Description>Unique identifier for the user profile</Description>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_name">
          <Type>nvarchar</Type>
          <MaxLength>200</MaxLength>
          <DisplayName>Display Name</DisplayName>
          <Description>User display name</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <IsPrimaryName>1</IsPrimaryName>
        </attribute>

        <attribute PhysicalName="eap_azure_ad_id">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Azure AD ID</DisplayName>
          <Description>Azure AD object ID for the user</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_email">
          <Type>nvarchar</Type>
          <MaxLength>256</MaxLength>
          <DisplayName>Email</DisplayName>
          <Description>User email address</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_department">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Department</DisplayName>
          <Description>User department</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_job_title">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Job Title</DisplayName>
          <Description>User job title</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_preferences">
          <Type>ntext</Type>
          <DisplayName>Preferences</DisplayName>
          <Description>JSON object with user preferences</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
        </attribute>

        <attribute PhysicalName="eap_default_client">
          <Type>lookup</Type>
          <DisplayName>Default Client</DisplayName>
          <Description>Default client for the user</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <LookupStyle>single</LookupStyle>
          <Targets>
            <Target>eap_Client</Target>
          </Targets>
        </attribute>

        <attribute PhysicalName="eap_last_login">
          <Type>datetime</Type>
          <DisplayName>Last Login</DisplayName>
          <Description>Timestamp of last login</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>DateAndTime</Format>
        </attribute>

        <attribute PhysicalName="eap_session_count">
          <Type>int</Type>
          <DisplayName>Session Count</DisplayName>
          <Description>Total number of sessions</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <MinValue>0</MinValue>
          <MaxValue>2147483647</MaxValue>
          <DefaultValue>0</DefaultValue>
        </attribute>

        <attribute PhysicalName="eap_is_active">
          <Type>bit</Type>
          <DisplayName>Is Active</DisplayName>
          <Description>Whether the user profile is active</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <DefaultValue>1</DefaultValue>
        </attribute>

        <attribute PhysicalName="createdon">
          <Type>datetime</Type>
          <DisplayName>Created On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="modifiedon">
          <Type>datetime</Type>
          <DisplayName>Modified On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statecode">
          <Type>state</Type>
          <DisplayName>Status</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statuscode">
          <Type>status</Type>
          <DisplayName>Status Reason</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>
      </attributes>
    </entity>
  </EntityInfo>
</Entity>
