<?xml version="1.0" encoding="utf-8"?>
<Entity xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Name>eap_config</Name>
  <EntityInfo>
    <entity Name="eap_config">
      <DisplayName>Configuration</DisplayName>
      <DisplayCollectionName>Configurations</DisplayCollectionName>
      <Description>Stores MCMAP platform configuration settings and feature flags.</Description>
      <OwnershipTypeMask>OrganizationOwned</OwnershipTypeMask>
      <IsActivity>false</IsActivity>
      <IsAvailableOffline>true</IsAvailableOffline>
      <IsCustomizable>1</IsCustomizable>
      <IsRenameable>1</IsRenameable>
      <CanCreateAttributes>1</CanCreateAttributes>
      <CanCreateForms>1</CanCreateForms>
      <CanCreateViews>1</CanCreateViews>
      <CanModifyAdditionalSettings>1</CanModifyAdditionalSettings>
      <PrimaryIdAttribute>eap_configid</PrimaryIdAttribute>
      <PrimaryNameAttribute>eap_name</PrimaryNameAttribute>
      <IntroducedVersion>7.0.0.0</IntroducedVersion>
      <localizedNames>
        <localizedName description="Configuration" languagecode="1033" />
      </localizedNames>
      <Descriptions>
        <Description description="Stores MCMAP platform configuration settings" languagecode="1033" />
      </Descriptions>
      <attributes>
        <attribute PhysicalName="eap_configid">
          <Type>primarykey</Type>
          <DisplayName>Config ID</DisplayName>
          <Description>Unique identifier for the configuration</Description>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_name">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Config Name</DisplayName>
          <Description>Name of the configuration setting</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <IsPrimaryName>1</IsPrimaryName>
        </attribute>

        <attribute PhysicalName="eap_key">
          <Type>nvarchar</Type>
          <MaxLength>100</MaxLength>
          <DisplayName>Config Key</DisplayName>
          <Description>Unique key for the configuration</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_value">
          <Type>ntext</Type>
          <DisplayName>Config Value</DisplayName>
          <Description>Value of the configuration (can be JSON)</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <Format>Text</Format>
        </attribute>

        <attribute PhysicalName="eap_category">
          <Type>picklist</Type>
          <DisplayName>Category</DisplayName>
          <Description>Category of the configuration</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="platform" />
              <Option value="100000001" label="agent" />
              <Option value="100000002" label="integration" />
              <Option value="100000003" label="feature_flag" />
              <Option value="100000004" label="security" />
              <Option value="100000005" label="telemetry" />
              <Option value="100000006" label="databricks" />
            </Options>
          </OptionSet>
        </attribute>

        <attribute PhysicalName="eap_environment">
          <Type>picklist</Type>
          <DisplayName>Environment</DisplayName>
          <Description>Environment this config applies to</Description>
          <RequiredLevel>required</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <OptionSet>
            <Options>
              <Option value="100000000" label="all" />
              <Option value="100000001" label="development" />
              <Option value="100000002" label="staging" />
              <Option value="100000003" label="production" />
            </Options>
          </OptionSet>
        </attribute>

        <attribute PhysicalName="eap_description">
          <Type>nvarchar</Type>
          <MaxLength>2000</MaxLength>
          <DisplayName>Description</DisplayName>
          <Description>Description of the configuration setting</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="eap_is_secret">
          <Type>bit</Type>
          <DisplayName>Is Secret</DisplayName>
          <Description>Whether this config contains sensitive data</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <DefaultValue>0</DefaultValue>
        </attribute>

        <attribute PhysicalName="eap_is_enabled">
          <Type>bit</Type>
          <DisplayName>Is Enabled</DisplayName>
          <Description>Whether this config is enabled</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
          <DefaultValue>1</DefaultValue>
        </attribute>

        <attribute PhysicalName="eap_version">
          <Type>nvarchar</Type>
          <MaxLength>20</MaxLength>
          <DisplayName>Version</DisplayName>
          <Description>Version of the configuration</Description>
          <RequiredLevel>none</RequiredLevel>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="createdon">
          <Type>datetime</Type>
          <DisplayName>Created On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="modifiedon">
          <Type>datetime</Type>
          <DisplayName>Modified On</DisplayName>
          <IsCustomizable>0</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statecode">
          <Type>state</Type>
          <DisplayName>Status</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>

        <attribute PhysicalName="statuscode">
          <Type>status</Type>
          <DisplayName>Status Reason</DisplayName>
          <IsCustomizable>1</IsCustomizable>
        </attribute>
      </attributes>
    </entity>
  </EntityInfo>
</Entity>
